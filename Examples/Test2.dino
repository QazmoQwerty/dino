import "DinoStd/std"

namespace NS {
    
    type Foo {
        int n
        int N {
            get: return n
            set: n ≡ value
        }
    }

    void Main() {
        var f1 ≡ new Foo
        f1.N ≡ 10
        any n ≡ f1
        if n is Foo {
            var f2 ≡ n as Foo
            Std.PrintD(f2.N)
            f2.N ≡ 20
            Std.PrintD(f2.N)
        }
    }
}