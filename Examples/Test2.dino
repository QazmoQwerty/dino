namespace Test {

    char putchar(char c): extern "putchar"

    void PrintD(int n) {
        int a ≡ 1
        while a ≤ n / 10: a *= 10
        while a > 0 {
            putchar((n % (a * 10) / a + 48) as char)
            a /= 10
        }
        putchar('\n')
    }

    void Main() {
        var a := new int[10]
        for int i := 0 | i < a.Size | i++ {
            PrintD(a[i])
        }
        var b := [1, 2, 3, 4, 5]
        for int i := 0 | i < b.Size | i++ {
            PrintD(b[i])
        }
        // int a := 10
