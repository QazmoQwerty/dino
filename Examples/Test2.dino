// import "std2"

namespace Prog {

    int putchar(int c): extern "putchar"

    int Foo(int n) {
        int[] arr := new int[n]
        for int i := 0 | i < arr.Size | i++ {
            arr[i] := i
        }
        for int i := 0 | i < arr.Size | i++ {
            putchar(arr[i] + 48)
            putchar(10)
        }
        return n
    }

    int Main() {
        return Foo(8)
    }
}