type Rectangle {
	public int h
	public int w

	int Area() {
		return this.h * this.w
	}
	void Show() {
		Print("height:", h, "width:", w)
	}
	void Enlarge(int n) {
		h += n
		w += n
	}
}

Rectangle CreateSquare(int len) {
	Rectangle rec
	rec.h = rec.w = len
	return rec
}

Rectangle rec = CreateSquare(2)
rec.h = 1
rec.w = 1
rec.Enlarge(2)
rec.Show()