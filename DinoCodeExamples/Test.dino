int Mul(int a, int b) {
    return a * b
}

int Pow(int a, int n) {
    int ret := 1
    for int i := 0 | i < n | i += 1:
        ret := Mul(ret, a)
    return ret
}

int Main() {
    //int i := Pow(2, 10)
    int i := 10
    do {
        if i = 10:
            i += 10
    } while i < 0
    return i
}